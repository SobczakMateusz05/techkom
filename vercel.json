{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [
      { "src": "/(.*)",  "dest": "/api/about.php" },
      { "src": "/(.*)",  "dest": "/api/cart.php" },
      { "src": "/(.*)",  "dest": "/api/connect.php" },
      { "src": "/(.*)",  "dest": "/api/contact.php" },
      { "src": "/(.*)",  "dest": "/api/function.php" },
      { "src": "/(.*)",  "dest": "/api/index.php" },
      { "src": "/(.*)",  "dest": "/api/login.php" },
      { "src": "/(.*)",  "dest": "/api/ofert.php" },
      { "src": "/(.*)",  "dest": "/api/opinion.php" },
      { "src": "/(.*)",  "dest": "/api/order.php" },
      { "src": "/(.*)",  "dest": "/api/orderhistory.php" },
      { "src": "/(.*)",  "dest": "/api/products.php" },
      { "src": "/(.*)",  "dest": "/api/promotion.php" },
      { "src": "/(.*)",  "dest": "/api/reclamation.php" },
      { "src": "/(.*)",  "dest": "/api/spec.php" }
    ]
}
